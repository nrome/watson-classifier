                    <item>redecorating</item>
                </grammar>
			</value>
			<value name="sophisticated" value="sophisticated">
                <grammar>
                    <item>discriminating</item>
                    <item>opera</item>
                    <item>museum</item>
                    <item>museums</item>
                    <item>theater</item>
                    <item>the arts</item>
                </grammar>
			</value>
			<value name="dependable" value="dependable">
                <grammar>
                    <item>rely on me</item>
                    <item>count on me</item>
